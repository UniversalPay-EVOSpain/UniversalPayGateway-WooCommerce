msgstr "Nombre del comercio"

#: class-wc-universalpay-gateway.php:157
msgid "Trade number"
msgstr "Número de comercio"

#: class-wc-universalpay-gateway.php:162
msgid "Terminal number"
msgstr "Número de terminal"

#: class-wc-universalpay-gateway.php:167
msgid "Secret key"
msgstr "Clave secreta de encriptación"

#: class-wc-universalpay-gateway.php:169
msgid "Encryptation Secret Key."
msgstr "Clave de encriptación secreta"

#: class-wc-universalpay-gateway.php:173
msgid "Sort Code"
msgstr "Código"

#: class-wc-universalpay-gateway.php:176
msgid "Complete"
msgstr "Completado"

#: class-wc-universalpay-gateway.php:177
msgid "SHA1 - Complete Extended"
msgstr "Firma SHA1 completa y extendida"

#: class-wc-universalpay-gateway.php:183
msgid "Url"
msgstr "URL"

#: class-wc-universalpay-gateway.php:186
msgid "Sermepa"
msgstr "Sermepa"

#: class-wc-universalpay-gateway.php:187
